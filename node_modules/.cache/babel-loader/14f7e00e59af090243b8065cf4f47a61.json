AAW,EAAV;6BAAb,YAAA,CAGM,KAHN,EAGM;AAHoB,UAAA,GAAG,EAAE,CAGzB;AAH4B,UAAA,KAAK,EAAC;AAGlC,SAHN,E,CACE,YAAA,CAAkD,KAAlD,EAAkD;AAA5C,UAAA,KAAK,EAAA,CAAE,CAAF,EAAW,cAAX;AAAuC,SAAlD,E,iBAAwC,C,CAAxC,EAAyC;AAAA;AAAzC,S,EACA,YAAA,CAA4C,KAA5C,EAA4C,IAA5C,EAA4C,gBAAA,CAApC,QAAA,CAAA,WAAA,CAAY,KAAA,CAAA,WAAA,CAAY,CAAZ,CAAZ,CAAoC,CAA5C,EAAiC;AAAA;AAAjC,S,CAFF,C;OAGM,CAHN,E;;AAAA,OAQI,GAJJ,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,UAEI,EADJ,YAAA,CAAiD,KAAjD,EAAiD,IAAjD,EAAiD,gBAAA,CAAzC,QAAA,CAAA,YAAA,CAAa,KAAA,CAAA,WAAA,CAAY,MAAzB,CAAyC,CAAjD,EAAuC;AAAA;AAAvC,OACI,CAHN,CAII,CAVN,C,qCAUM,CAVN;AAAA,K;;;;GANF,E;;AAAA,I,kCAAA,CAkBI,CAtDN,E;;AAAA,GAuDI,CAzDN,C","sourcesContent":["<template>\n<div>\n  <ChartTitle>Estimated impact of the COVID-19 outbreak on global tech shipments in Q1 2020</ChartTitle>\n  <div class=\"chart-body\" ref=\"container\">\n    <svg\n      :viewBox=\"`0 0 ${width} ${height}`\"\n      ref=\"svg\"\n    >\n      <ChartXAxis\n        :x=\"x\"\n        :tick-values=\"groups\"\n        :transform=\"`translate(0,${height - margin.bottom})`\"\n      />\n      <ChartYAxis\n        :y=\"y\"\n        :transform=\"`translate(${margin.left},0)`\"\n      />\n      <g>\n        <g \n          v-for=\"(g, i) in groups\" \n          :key=\"g\"\n          :transform=\"`translate(${x(g)},0)`\"\n          class=\"hoverable\"\n          @mouseenter=\"entered(i)\"\n          @mouseleave=\"left\"\n          >\n            <rect\n              v-for=\"s in series\"\n              :key=\"s\"\n              :class=\"s\"\n              :x=\"xInner(s)\"\n              :width=\"xInner.bandwidth()\"\n              :y=\"y(records[i][s])\"\n              :height=\"y.range()[0] - y(records[i][s])\"\n              fill=\"currentColor\"\n            ></rect>\n        </g>\n      </g>\n    </svg>\n    <ChartTooltip\n      :bounds-width=\"width\"\n      :x=\"tooltipX\"\n      :y=\"tooltipY\"\n      :show=\"tooltipShow\"\n    >\n      <div v-if=\"tooltipData\">\n        <div class=\"tooltip-item tooltip-title\">{{ tooltipData.device }}</div>\n        <div v-for=\"s in series\" :key=\"s\" class=\"tooltip-item\">\n          <div :class=\"s\" class=\"tooltip-name\">{{ s }}</div>\n          <div>{{ formatValue(tooltipData[s]) }}</div>\n        </div>\n        <div class=\"tooltip-item\">\n          <div class=\"tooltip-name change\">Change</div>\n          <div>{{ formatChange(tooltipData.change) }}</div>\n        </div>\n      </div>\n    </ChartTooltip>\n  </div>\n</div>\n</template>\n\n<script>\nimport { json, max, scaleBand, scaleLinear } from \"d3\";\nimport ChartTitle from \"./ChartTitle.vue\";\nimport ChartXAxis from \"./ChartXAxis.vue\";\nimport ChartYAxis from \"./ChartYAxis.vue\";\nimport ChartTooltip from \"./ChartTooltip.vue\";\n\nexport default {\n  components: {\n    ChartTitle,\n    ChartXAxis,\n    ChartYAxis,\n    ChartTooltip\n  },\n  data() {\n    return {\n      groups: [],\n      series: [],\n      records: [],\n      maxValue: 0,\n      width: 300,\n      height: 400,\n      margin: {\n        top: 10,\n        right: 5,\n        bottom: 120,\n        left: 40\n      },\n      tooltipData: null,\n      tooltipY: 100,\n      tooltipShow: false\n    }\n  },\n  computed: {\n    x() {\n      return scaleBand()\n        .domain(this.groups)\n        .range([this.margin.left, this.width - this.margin.right])\n        .padding(0.4);\n    },\n    xInner() {\n      return scaleBand()\n        .domain(this.series)\n        .range([0, this.x.bandwidth()])\n        .padding(0.1);\n    },\n    y() {\n      return scaleLinear()\n        .domain([0, this.maxValue])\n        .range([this.height - this.margin.bottom, this.margin.top])\n        .nice();\n    },\n    formatValue() {\n      return v => `${v}M units`;\n    },\n    formatChange() {\n      return v => `${v}%`;\n    },\n    tooltipX() {\n      return this.tooltipData !== null \n        ? this.x(this.tooltipData.device) + this.x.bandwidth() \n        : -999;\n    }\n  },\n  created() {\n    json('hardware_manufacturing.json')\n    .then(data => {\n      this.groups = data.map(d => d.device);\n      this.series = [\"previous\", \"revised\"];\n      this.records = data;\n      this.maxValue = max(data, d => max(this.series, s => d[s]));\n    });\n  },\n  mounted() {\n    this.resize();\n    window.addEventListener(\"resize\", this.resize);\n  },\n  beforeUnmount() {\n    window.removeEventListener(\"resize\", this.resize);\n  },\n  methods: {\n    resize() {\n      this.width = this.$refs.container.clientWidth;\n    },\n    entered(i) {\n      this.tooltipShow = true;\n      this.tooltipData = this.records[i];\n    },\n    left() {\n      this.tooltipShow = false;\n    }\n  }\n}\n</script>\n\n<style scoped>\n.chart-body {\n  position: relative;\n}\n.previous {\n  color: #9146FF;\n}\n.revised {\n  color: #F92772;\n}\n.change {\n  color: #C1B761;\n}\n.hoverable:hover {\n  filter: brightness(125%);\n}\n</style>"],"sourceRoot":""}]}