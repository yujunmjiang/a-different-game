CAAf,CAArB,IAA0C,EAAvD,CADA,CAC2D;AAD3D,QAEA,KAAK,CAAL,CAAO,MAAP,EAJJ,CADM,CAKc;AACrB,KAPO;AAQR,IAAA,SARQ,uBAQI;AAAA;;AACV,aAAO,KAAK,CAAL,CAAO,KAAP,GACH,UAAA,CAAA;AAAA,eAAK,KAAI,CAAC,CAAL,CAAO,CAAP,CAAL;AAAA,OADG,CACY;AADZ,QAEH,KAAK,CAAL,CAAO,YAAP,GACA,UAAA,CAAA;AAAA,eAAK,KAAI,CAAC,CAAL,CAAO,CAAP,IAAY,KAAI,CAAC,CAAL,CAAO,SAAP,KAAqB,CAAtC;AAAA,OADA,CACwC;AADxC,QAEA,UAAA,CAAA;AAAA,eAAK,KAAI,CAAC,CAAL,CAAO,CAAP,IAAY,KAAI,CAAC,CAAL,CAAO,IAAP,KAAgB,CAAjC;AAAA,OAJJ,CADU,CAK6B;AACzC;AAdQ;AAfG,CAAf","sourcesContent":["<template>\n  <g class=\"axis axis--x\">\n    <path\n      class=\"domain\"\n      :d=\"`M${x.range()[0]},10V0H${x.range()[1]}V10`\"\n      fill=\"none\"\n      stroke=\"currentColor\"\n    ></path>\n    <g v-for=\"t in ticks\" \n      :key=\"t\"\n      :transform=\"`translate(${translate(t)},0)`\"\n      class=\"tick\"\n    >\n      <line\n        y2=\"10\"\n        stroke=\"currentColor\"\n      ></line>\n      <text\n        transform=\"translate(0,15)rotate(-60)\"\n        dy=\"0.32em\"\n        text-anchor=\"end\"\n        fill=\"currentColor\"\n      >\n        {{ tickFormat(t) }}\n      </text>\n    </g>\n  </g>\n</template>\n\n<script>\n  export default {\n    props: {\n      x: {\n        type: Function,\n        required: true \n      },\n      tickValues: {\n        type: Array,\n        default: () => []\n      },\n      tickFormat: {\n        type: Function,\n        default: d => d\n      }\n    },\n    computed: {\n      ticks() {\n        return this.tickValues.length\n          ? this.tickValues // Custom defined tick values\n          : this.x.ticks \n          ? this.x.ticks((this.x.range()[1] - this.x.range()[2]) / 80) // Continouous scale\n          : this.x.domain() // Ordinal scale\n      },\n      translate() {\n        return this.x.ticks\n          ? t => this.x(t) // Continuous scale\n          : this.x.paddingInner\n          ? t => this.x(t) + this.x.bandwidth() / 2 // Band scale\n          : t => this.x(t) + this.x.step() / 2 // Point scale\n      }\n    },\n  }\n</script>\n\n<style scoped>\n.axis {\n  font-size: 12px;\n  line-height: 28px;\n}\n</style>"],"sourceRoot":""}]}